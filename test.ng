fn i64 fibonacci(i64 n)                            
{     	                                    
    if (n == 1) {
        return 1;
    }
    else {                                    
        if (n == 2) {
            return 1;
        }
        else {
            return fibonacci(n - 1) + fibonacci(n - 2);
        }
    }
}

fn i32 main()
{
    i64 n;
    i64 result;
    printf("Digite um n√∫mero: ");
    read(n);
    printf("Numero de Fibonacci:\n");
    result = fibonacci(n);
    printf("%d\n", result);
    return 0;
}
